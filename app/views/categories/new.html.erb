<%= render 'shared/content_title', title: 'New Category' %>
<%= render 'shared/error', obj: @category %>

<div class='well'>
    <%= form_for @category do |f| %>
    <div class='control-group'>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <%= f.submit 'Add Category', class: 'btn btn-primary' %>
    <% end %>
    
</div> 