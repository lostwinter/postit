<%= render 'shared/content_title', title: 'Login' %>

<div class='well'>
    <%= form_tag '/login' do %>
        <div class='control-group'>
           <%= label_tag :username %>  
           <%= text_field_tag :username %>
           </br>
           <%= label_tag :password %>
           <%= password_field_tag :password %>
           </br>
           <%= submit_tag 'Login', class: 'btn btn-success' %>
        </div>
    <% end %>
</div>